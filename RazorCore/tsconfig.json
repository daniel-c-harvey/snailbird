{
  "compileOnSave": true,
  "compilerOptions": {
    "target": "es2015",
    "module": "es2015",
    "moduleResolution": "node",
    "strict": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "forceConsistentCasingInFileNames": true,
    "outDir": "wwwroot/js",
    "rootDir": "wwwroot/js",
    "sourceMap": true
  },
  "include": [ "wwwroot/js/**/*.ts" ],
  "exclude": [ "node_modules", "wwwroot/js/**/*.js" ]
}