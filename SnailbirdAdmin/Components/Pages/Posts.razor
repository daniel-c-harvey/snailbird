@using RazorCore.Navigation;
@using SnailbirdAdmin.Models;

@page "/posts"

<PageTitle>Posts</PageTitle>

<NavPanel TMode="PostManagerMode" GetContext="GetPostManager">
    <PostManager @ref="postManager"/>
</NavPanel>

@code{
    private PostManager postManager;

    private PostManager GetPostManager()
    {
        return postManager;
    }
}